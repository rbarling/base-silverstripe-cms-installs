{
    "name": "silverstripe/installer",
    "description": "The SilverStripe Framework Installer",
    "require": {
        "php": ">=5.3.3",
        "silverstripe/cms": "3.7.0@stable",
        "silverstripe/framework": "3.7.0@stable",
        "silverstripe/reports": "3.7.0@stable",
        "silverstripe/siteconfig": "3.7.0@stable",
        "silverstripe-themes/simple": "3.1.*",
        "silverstripe/crontask": "1.2",
        "silverstripe/dynamodb": "3.0.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^3 || ^4 || ^5"
    },
    "extra": [],
    "config": {
        "process-timeout": 600,
        "allow-plugins": {
            "composer/installers": true
        }
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
